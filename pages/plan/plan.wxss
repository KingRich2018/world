.all{
  min-height:100%;
  background: #e6e6e6;
}

.open{
  width:30px;
  height: 30px;
  margin:6% 0 0 20px;
}
.openCont{
  width: 40%;
  height: 150px;
  margin-left:20px; 
  background-color: #FFF;
  border-radius: 15px;
  z-index: 999;
  position: fixed;
}
.noData{
  width: 180px;
  margin:120px auto;
}
.noDataImg{
  width: 180px;
  height: 200px;
}
.textnoData{
  display: block;
  font-size: 12px;
  color: gray;
  margin-top:10px; 
  text-align: center;

}
.modalDlg {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  background-color:#eee;
  opacity: 0.5;
  top: 0%;
}
.plan{
  width: 30px;
  height: 30px;
  float: left;
  margin: 14px 0 0 10px; 
}
.textPlan{
  font-size: 14px;
  display: block;
  float: left;
  padding-left:10px; 
  margin-top:20px;
}
.clear{
  clear:both;
}
.main{
  width: 40%;
  height: 180px;
  background-color: #e6e6e6;
  opacity: 0.8;
  border-radius: 10px;
  margin:10px 5%;
  float: left;
  
   box-shadow: inset 0 0 0 1px rgba(58, 153, 216, 0.1), inset 0 0em 0
		rgba(94, 136, 163, 0.2), 0 10px 10px rgba(58, 153, 216, 0.05);
}
.flag{
  display: block;
  width:45px;
  height: 45px;
  padding-top:10px;
  margin:10px auto;
}
.title{
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 10px;
  font-size: 16px;
  font-weight: 700;
}
.dao{
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: 700;
  font-size: 16px;
}
.startTime{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 14px;
  
 
}
.endTime{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 14px;
  
}